    "outDir": "dist",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "emitDeclarationOnly": false,

    "module": "ESNext",
    "target": "ES2020",
    "jsx": "react-jsx",
    "moduleResolution": "Bundler",

    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src"]
}
